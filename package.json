{
  "name": "@codewell/render-gate",
  "version": "0.0.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codewell/react-render-gate.git"
  },
  "homepage": "https://github.com/codewell/react-render-gate",
